[
  {
    "$id": "ui-container",
    "Default": {
      "BackgroundColor": [ "#00000000" ],
      "BackgroundShader": "shader-ui"
    }
  },
  {
    "$id": "ui-icon",
    "Default": {
      "BackgroundColor": [ "#ffffff88" ],
      "BackgroundShader": "shader-ui"
    },
    "States": [
      {
        "State": [ "Hover" ],
        "Attributes": {
          "BackgroundColor": [ "#ffffffaa" ]
        }
      }
    ]
  },
  {
    "$id": "ui-element",
    "Default": {
      "BackgroundColor": [ "#00000088" ],
      "BorderColor": [ "#ffffff88" ],
      "BorderWidth": [ 1 ],
      "BackgroundShader": "shader-ui",
      "FontFace": "font-abel",
      "Color": "#ffffff88",
      "Shader": "shader-default",
      "DisableScissor": true
    },
    "States": [
      {
        "State": [ "Hover" ],
        "Attributes": {
          "BackgroundColor": [ "#000000aa" ],
          "BorderColor": [ "#ffffffaa" ],
          "Color": "#ffffffaa"
        }
      },
      {
        "State": [ "Toggle" ],
        "Attributes": {
          "BackgroundColor": [ "#ff880088" ]
        }
      },
      {
        "State": [ "Toggle", "Hover" ],
        "Attributes": {
          "BackgroundColor": [ "#ff8800aa" ]
        }
      }
    ]
  },
  {
    "$id": "ui-button",
    "Parent": "ui-element",
    "Default": {
      "BackgroundColor": [ "#ffffff88" ]
    },
    "States": [
      {
        "State": [ "Hover" ],
        "Attributes": {
          "BackgroundColor": [ "#ffffffaa" ]
        }
      }
    ]
  },
  {
    "$id": "ui-element-basic",
    "Parent": "ui-element",
    "Default": {
      "CornerRadius": [
        [ 12, 12 ]
      ],
      "FontSize": 14,
      "Padding": [ 8 ],
      "Margin": [ 2 ]
    }
  },
  {
    "$id": "ui-element-internal",
    "Parent": "ui-container",
    "Default": {
      "BackgroundColor": [ "#00000000" ],
      "BackgroundShader": "shader-ui",
      "FontFace": "font-abel",
      "Color": "#ffffff88",
      "Shader": "shader-default",
      "DisableScissor": true
    },
    "States": [
      {
        "State": [ "Hover" ],
        "Attributes": {
          "Color": "#ffffffaa"
        }
      }
    ]
  },
  {
    "$id": "pane-standard",
    "Parent": "ui-element",
    "Default": {
      "Size": [ 900, 700 ],
      "Padding": [ 4 ],
      "CornerRadius": [
        [ 16, 16 ]
      ]
    }
  },
  {
    "$id": "pane-standard-title",
    "Parent": "ui-element",
    "Default": {
      "Size": [ 812, 64 ],
      "Padding": [ 16 ],
      "Margin": [ 4 ],
      "CornerRadius": [
        [ 8, 8 ]
      ],
      "FontSize": 32
    }
  },
  {
    "$id": "pane-standard-close",
    "Parent": "ui-button",
    "Default": {
      "Size": [ 64, 64 ],
      "Margin": [ 4 ],
      "CornerRadius": [
        [ 8, 8 ]
      ],
      "Texture": "icon-x"
    }
  },
  {
    "$id": "pane-tab-container",
    "Parent": "ui-container",
    "Default": {
      "Size": [ 884, 32 ],
      "Padding": [ 2 ],
      "Margin": [ 4 ],
      "DisableScissor": true
    }
  },
  {
    "$id": "pane-tab-option",
    "Parent": "ui-element-basic",
    "Default": {
      "Size": [ 128, 28 ],
      "Align": "Center"
    }
  },
  {
    "$id": "pane-body",
    "Parent": "ui-container",
    "Default": {
      "Size": [ 884, 580 ]
    }
  },
  {
    "$id": "pane-action-button",
    "Parent": "ui-button",
    "Default": {
      "Size": [ 28, 28 ],
      "CornerRadius": [ [ 4, 4 ] ]
    }
  }
]