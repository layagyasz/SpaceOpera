[
  {
    "$id": "ui-container",
    "Default": {
      "BackgroundColor": [ "#00000000" ],
      "BackgroundShader": "shader-ui"
    }
  },
  {
    "$id": "ui-icon",
    "Default": {
      "BackgroundColor": [ "#ffffffcc" ],
      "BackgroundShader": "shader-ui"
    },
    "States": [
      {
        "State": [ "Hover" ],
        "Attributes": {
          "BackgroundColor": [ "#ffffff" ]
        }
      }
    ]
  },
  {
    "$id": "ui-element",
    "Default": {
      "BackgroundColor": [ "#00000088" ],
      "ForegroundColor": "#ffffffcc",
      "BorderColor": [ "#ffffffcc" ],
      "BorderWidth": [ 1 ],
      "BackgroundShader": "shader-ui",
      "FontFace": "font-abel",
      "Color": "#ffffffcc",
      "Shader": "shader-default",
      "DisableScissor": true
    },
    "States": [
      {
        "State": [ "Hover" ],
        "Attributes": {
          "BackgroundColor": [ "#000000" ],
          "ForegroundColor": "#ffffff",
          "BorderColor": [ "#ffffff" ],
          "Color": "#ffffff"
        }
      },
      {
        "State": [ "Toggle" ],
        "Attributes": {
          "BackgroundColor": [ "#ff8800cc" ]
        }
      },
      {
        "State": [ "Toggle", "Hover" ],
        "Attributes": {
          "BackgroundColor": [ "#ff8800" ]
        }
      }
    ]
  },
  {
    "$id": "ui-element-basic",
    "Parent": "ui-element",
    "Default": {
      "CornerRadius": [
        [ 12, 12 ]
      ],
      "FontSize": 14,
      "Padding": [ 8 ],
      "Margin": [ 2 ]
    }
  },
  {
    "$id": "ui-element-internal",
    "Parent": "ui-container",
    "Default": {
      "BackgroundColor": [ "#00000000" ],
      "BackgroundShader": "shader-ui",
      "FontFace": "font-abel",
      "Color": "#ffffffcc",
      "Shader": "shader-default",
      "DisableScissor": true
    },
    "States": [
      {
        "State": [ "Hover" ],
        "Attributes": {
          "Color": "#ffffff"
        }
      },
      {
        "State": [ "Toggle" ],
        "Attributes": {
          "BackgroundColor": [ "#ff8800cc" ]
        }
      },
      {
        "State": [ "Toggle", "Hover" ],
        "Attributes": {
          "BackgroundColor": [ "#ff8800" ]
        }
      }
    ]
  },
  {
    "$id": "pane-standard",
    "Parent": "ui-element",
    "Default": {
      "Size": [ 900, 700 ],
      "Padding": [ 4 ],
      "CornerRadius": [
        [ 16, 16 ]
      ]
    }
  },
  {
    "$id": "pane-standard-title",
    "Parent": "ui-element",
    "Default": {
      "Size": [ 812, 64 ],
      "Padding": [ 16 ],
      "Margin": [ 4 ],
      "CornerRadius": [
        [ 8, 8 ]
      ],
      "FontSize": 32
    }
  },
  {
    "$id": "pane-standard-close",
    "Parent": "ui-element",
    "Default": {
      "Size": [ 64, 64 ],
      "Margin": [ 4 ],
      "CornerRadius": [
        [ 8, 8 ]
      ],
      "Texture": "icon-x"
    }
  },
  {
    "$id": "pane-tab-container",
    "Parent": "ui-container",
    "Default": {
      "Size": [ 884, 32 ],
      "Padding": [ 2 ],
      "Margin": [ 4 ],
      "DisableScissor": true
    }
  },
  {
    "$id": "pane-tab-option",
    "Parent": "ui-element-basic",
    "Default": {
      "Size": [ 128, 28 ],
      "Align": "Center"
    }
  },
  {
    "$id": "pane-body",
    "Parent": "ui-container",
    "Default": {
      "Size": [ 892, 580 ]
    }
  },
  {
    "$id": "pane-body-no-tabs",
    "Parent": "ui-container",
    "Default": {
      "Size": [ 892, 620 ]
    }
  },
  {
    "$id": "pane-action-button",
    "Parent": "ui-element",
    "Default": {
      "Size": [ 28, 28 ],
      "CornerRadius": [ [ 4, 4 ] ]
    }
  }
]