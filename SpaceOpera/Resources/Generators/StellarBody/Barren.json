          "Key": "combine",
          "Channel": [ "Color" ],
          "Inputs": {
            "left": "position",
            "right": "denormalize"
          },
          "Parameters": {
            "LeftTransform": {
              "Type": "Cardamom.Utils.Suppliers.ConstantSupplier`1[[OpenTK.Mathematics.Matrix4, OpenTK.Mathematics]], Cardamom",
